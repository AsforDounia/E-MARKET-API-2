{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": ["eslint:recommended", "standard", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": ["warn"]
    }
}
